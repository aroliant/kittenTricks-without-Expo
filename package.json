{
  "name": "kittenTricks",
  "version": "2.0.0",
  "private": true,
  "author": "akveo",
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start:dev": "npm run env:set -- dev && npm start -- -c",
    "start:prod": "npm run env:set -- prod && npm start -- -c",
    "env:set": "./scripts/environment/set-env.sh",
    "build": "tsc -p ./tsconfig.json",
    "clean": "rimraf ./dist",
    "lint": "tslint -p ./tsconfig.json",
    "lint:fix": "npm run lint -- --fix",
    "ci:install": "npm ci",
    "ci:lint": "npm run lint",
    "ci:build": "npm run env:set -- prod && npm run build",
    "ios": "react-native run-ios",
    "android": "react-native run-android"
  },
  "dependencies": {
    "@eva-design/eva": "1.0.0",
    "react": "^16.8.3",
    "react-native": "0.62.3",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-keyboard-aware-scroll-view": "^0.8.0",
    "react-native-ui-kitten": "^4.0.0",
    "react-native-vector-icons": "^6.5.0",
    "react-navigation": "^3.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/runtime": "^7.1.2",
    "@types/react": "^16.8.19",
    "@types/react-native": "^0.57.60",
    "@types/react-navigation": "^3.0.7",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-preset-expo": "^5.2.0",
    "husky": "^1.1.2",
    "rimraf": "^2.6.2",
    "tslint": "^5.12.1",
    "typescript": "^3.5.1"
  }
}
